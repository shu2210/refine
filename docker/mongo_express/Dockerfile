FROM mongo-express

ENV ME_CONFIG_MONGODB_SERVER mongodb
ENV ME_CONFIG_MONGODB_PORT 27017
ENV ME_CONFIG_MONGODB_ENABLE_ADMIN false
ENV ME_CONFIG_MONGODB_AUTH_DATABASE log
ENV ME_CONFIG_MONGODB_AUTH_USERNAME logger
ENV ME_CONFIG_MONGODB_AUTH_PASSWORD logger
ENV ME_CONFIG_BASICAUTH_USERNAME logger
ENV ME_CONFIG_BASICAUTH_PASSWORD logger
